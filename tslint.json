{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-bitwise": false,
        "no-console": [
            true,
            "log"
        ],
        "arrow-parens": false,
        "curly": [
            true,
            "as-needed"
        ],
        "ordered-imports": false,
        "object-literal-key-quotes": [
            "consistent"
        ],
        "one-line": [
            false
        ],
        "interface-name": [
            false
        ],
        "max-classes-per-file": [
            1
        ]
    }
}